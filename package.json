{
  "name": "vispeech",
  "version": "0.0.0",
  "description": "thesic recognize voice vietnamese",
  "author": {
    "name": "k16 - 1612704 - 1612339",
    "email": "1612704@student.hcmus.edu.vn",
    "url": "http://www.vispeech.com"
  },
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && tsc && rm -rf ./apps/build && npm run build --prefix ./apps",
    "build:docker": "docker build --force-rm -t vispeech .",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "npm run build --prefix ./apps && nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "npm run build --scripts-prepend-node-path",
    "start:prod": "node dist/src/main.js",
    "start:prod:analyze": "npm run prestart:prod && 0x dist/src/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^6.11.6",
    "@nestjs/core": "^6.11.6",
    "@nestjs/cqrs": "^6.1.0",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/passport": "^6.1.1",
    "@nestjs/platform-fastify": "^6.11.6",
    "@nestjs/serve-static": "^1.0.2",
    "@nestjs/swagger": "^4.2.6",
    "@nestjs/typeorm": "^6.2.0",
    "babel-jest": "24.9.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "dotenv": "^6.0.0",
    "fastify": "^2.12.0",
    "fastify-static": "^2.6.0",
    "fastify-swagger": "^2.5.0",
    "geteventstore-promise": "^3.1.1",
    "logform": "^2.1.2",
    "mongodb": "^3.5.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.2.2",
    "rxjs-compat": "^6.4.0",
    "strict-event-emitter-types": "^2.0.0",
    "stripe": "^8.29.0",
    "typeorm": "^0.2.22",
    "uuid": "^3.4.0",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "0x": "^4.7.3",
    "@nestjs/testing": "^5.1.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.9",
    "@types/node": "^10.7.1",
    "@types/supertest": "^2.0.5",
    "jest": "24.9.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "5.11.0",
    "typescript": "^3.7.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}